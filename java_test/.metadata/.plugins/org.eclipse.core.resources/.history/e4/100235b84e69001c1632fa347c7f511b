package test03;

import java.util.Scanner;

public class SearchArray {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("지정할 갯수를 입력하세요.");
		int a = sc.nextInt();
		
		int[] x = new int[a];
		for(int i=0; i<a; i++) {
			System.out.println("삽입할 정수를 입력하세요");
			int b = sc.nextInt();
			x += b;
		}
		
	}

}
