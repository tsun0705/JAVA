	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("개수를 입력하세요.");
		int num = sc.nextInt();
		int[] x = new int[num];
		
		for(int i=0; i<num; i++) {
			System.out.println("정수를 입력하세요");
			x[i] = sc.nextInt();
		}
		
		Scanner sc2 = new Scanner(System.in);
		System.out.println("개수를 입력하세요.");
		int num2 = sc2.nextInt();
		int[] x2 = new int[num];

		for(int i=0; i<num; i++) {
			System.out.println("정수를 입력하세요");
			x2[i] = sc2.nextInt();
		}
		
	}

